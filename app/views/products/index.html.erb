<h1>Birds</h1>
<% cache(cache_key_for_products) do %>
  <div>
    <div class="row">
      <% @products.each do |product| %>
        <%= render product %>
      <% end %>
    </div>
  </div>
<% end %>
<% if signed_in? && current_user.admin? %>
  <%= link_to 'New Product', new_product_path, class: "admincreate_text" %>
<% end %>
<div class="page_bar">
  <%= will_paginate @products %>
</div>
