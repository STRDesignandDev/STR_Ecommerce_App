<h1>Birds</h1>

<div>
  <div class="row">
    <% @products.each do |product| %>
      <%= render product %>
    <% end %>
  </div>
</div>
<% if signed_in? && current_user.admin? %>
  <%= link_to 'New Product', new_product_path, class: "admincreate_text" %>
<% end %>
<div class="page_bar">
  <p>More Birds</p>
  <%= will_paginate @products %>
</div>
